{
  "name": "nightwatchjs-workshop",
  "version": "1.0.0",
  "description": "It is just a demo",
  "main": "nightwatch.conf.js",
  "scripts": {
    "test": "nightwatch --retries 2",
    "test-chrome": "nightwatch --env chrome --retries 2",
